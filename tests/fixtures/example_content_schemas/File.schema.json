{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "description": "A file is an object that contains information generated from a process, either an Experiment or an Analysis.",
  "properties": {
    "checksum": {
      "type": "string"
    },
    "filename": {
      "type": "string"
    },
    "format": {
      "type": "string"
    },
    "size": {
      "type": "integer"
    }
  },
  "required": [
    "filename",
    "format",
    "checksum",
    "size"
  ],
  "type": "object"
}
